{
  "entities": [],
  "rules": [
    {
      "from": "#poisoned",
      "where": {
        "filter": [
          { "var": "entities" },
          { ">": [{ "var": "properties.health" }, 0] }
        ]
      },
      "do": {
        "mut": {
          "map": [
            { "var": "entities" },
            {
              "merge": [
                { "var": "" },
                { "properties": { "health": 0 } }
              ]
            }
          ]
        }
      }
    }
  ]
}
