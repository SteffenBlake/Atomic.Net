{
  "entities": [
    {
      "id": "ui-element",
      "tags": ["ui"],
      "transform": {
        "anchor": { "x": 0, "y": 0, "z": 0 }
      }
    }
  ],
  "rules": [{
    "from": "#ui",
    "where": { "var": "entities" },
    "do": {
      "mut": [
        {
          "target": { "transform": "anchor" },
          "value": ["not", "a", "vector"]
        }
      ]
    }
  }]
}
