{
  "entities": [
    {
      "id": "ui-element",
      "tags": ["ui"],
      "transform": {
        "anchor": [0, 0, 0]
      }
    }
  ],
  "rules": [{
    "from": "#ui",
    "where": { "var": "entities" },
    "do": {
      "mut": [
        {
          "target": { "transform": { "anchor": true } },
          "value": { "x": 0.5, "y": 0.5, "z": 0 }
        }
      ]
    }
  }]
}
