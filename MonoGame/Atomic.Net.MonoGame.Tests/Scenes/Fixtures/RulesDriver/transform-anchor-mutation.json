{
  "entities": [
    {
      "id": "ui-element",
      "tags": [
        "ui"
      ],
      "transform": {
        "anchor": {
          "x": 0,
          "y": 0,
          "z": 0
        }
      }
    }
  ],
  "rules": [
    {
      "from": "#ui",
      "where": {
        "var": "entities"
      },
      "do": {
        "mut": [
          {
            "target": {
              "anchor": "x"
            },
            "value": 0.5
          },
          {
            "target": {
              "anchor": "y"
            },
            "value": 0.5
          },
          {
            "target": {
              "anchor": "z"
            },
            "value": 0
          }
        ]
      }
    }
  ]
}