{
  "entities": [{"id": "item", "tags": ["positioned"]}],
  "rules": [{
    "from": "#positioned",
    "where": { "var": "entities" },
    "do": {
      "mut": [
        {"target": { "flexPositionLeft": "value" }, "value": 50.0},
        {"target": { "flexPositionLeft": "percent" }, "value": false}
      ]
    }
  }]
}
