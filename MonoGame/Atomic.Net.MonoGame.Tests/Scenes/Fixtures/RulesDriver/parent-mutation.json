{
  "entities": [
    {
      "id": "parent-node",
      "tags": ["parent"]
    },
    {
      "id": "child-node",
      "tags": ["child"]
    }
  ],
  "rules": [{
    "from": "#child",
    "where": { "var": "entities" },
    "do": {
      "mut": [
        {
          "target": {"parent": true},
          "value": "@parent-node"
        }
      ]
    }
  }]
}
