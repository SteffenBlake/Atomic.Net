{
  "entities": [
    {
      "id": "testEntity",
      "properties": {
        "value": 0
      }
    }
  ],
  "sequences": [
    {
      "id": "tween-simple",
      "steps": [
        {
          "tween": {
            "from": 0,
            "to": 100,
            "duration": 1.0,
            "do": {
              "mut": [
                {
                  "target": { "properties": "value" },
                  "value": 99
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "rules": [
    {
      "from": "@testEntity",
      "where": {
        "filter": [
          { "var": "entities" },
          { "==": [1, 1] }
        ]
      },
      "do": {
        "sequenceStart": "tween-simple"
      }
    }
  ]
}
